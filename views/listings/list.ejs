<%- layout("/layouts/boilerplate.ejs") %>
<body>
    <h3>list details</h3>
    <ul>
        <li><%= list.title %></li>
        <li><%= list.description %></li>
        <li><%= list.price.toLocaleString("en-IN") %></li>
        <li><%= list.location %></li>
        <li><%= list.country %></li>
      
    </ul>
    <br>
    <a href="/listings/<%=list._id%>/edit">Edit list</a>
    <form action="/listings/<%=list._id%>?_method=DELETE" method="post">
        <button>delete</button>
    </form>
</body>
